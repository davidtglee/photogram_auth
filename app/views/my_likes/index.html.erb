<div class="page-header">
  <h1>
    My likes
    <a href="/my_likes/new" class="btn btn-lg btn-success">New My like</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Actions</th>
      </tr>

      <% @my_likes.each do |my_like| %>
      <tr>
        <td>
          <a href="/my_likes/<%= my_like.id %>" class="btn btn-primary">Show</a>
          <a href="/my_likes/<%= my_like.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_my_like/<%= my_like.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
